{
  "name": "zrobank",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "files": [
    "index.ts",
    "ts"
  ],
  "scripts": {    
    "sqlmigrate": "./bin/sql-migrate/run.sh",
    "sqlmigrate:homolog": "./bin/sql-migrate/run.sh up --env=homologation",
    "run:ts": "node --stack_trace_limit=100 -r trace -r clarify -r ts-node/register/transpile-only",
    "start": "node-dev --no-notify --stack_trace_limit=100 -r trace -r clarify -r ts-node/register/transpile-only src/index.ts app",
    "start:prod": "ENV_FILE_PATH=.env.prod node --stack_trace_limit=100 -r trace -r clarify -r ts-node/register/transpile-only src/index.ts app",
    "test": "NODE_ENV=test NODE_OPTIONS=--max_old_space_size=6144 jest --forceExit --silent --noStackTrace --maxWorkers=1",
    "test:spec": "NODE_ENV=test NODE_OPTIONS=--max_old_space_size=6144 SKIP_MIGRATION=1 jest --forceExit --silent --noStackTrace --testPathPattern=\".*\\.spec\\.ts\" --maxWorkers=1",
    "test:migrations": "node src/test/setup.js",
    "build": "yarn format:check && yarn install && rm -rf ./build && mkdir -p ./build && mkdir -p dist && cp -r src dist package.json yarn.lock ./build",
    "build:ts": "tslint -c tslint.json -p tsconfig.json && tsc",
    "ts:fix": "tslint --fix -c tslint.json -p tsconfig.json 'src/components/**/*{.ts,.tsx}'",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:once": "tslint --fix --force -p tsconfig.json './src/components/**/*.+(ts|tsx)'",
    "tslint:watch": "yarn tslint:once && onchange 'src/components/**/*.*' -- yarn tslint:once",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/components/**/*.{ts,tsx}\"",
    "hook:pre-push": "yarn format:check && yarn tslint"
  },
  "engines": {
    "node": "10.x"
  },
  "author": "Lucas <lucasrocha.wiix@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "argon2": "^0.27.0",
    "axios": "^0.18.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.17.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "commander": "^2.19.0",
    "cookie-parser": "^1.4.5",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "debug": "^4.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.3",
    "helmet": "^3.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "pg": "6.0.5",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.83.0",
