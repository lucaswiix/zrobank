    "@types/sequelize": "4.27.24",
    "@types/validator": "^13.1.0",
    "@vercel/node": "^1.7.1",
    "babel-jest": "^26.1.0",
    "clarify": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
